{
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "version": "2.0.1",
    "permissions": [
        "contextMenus",
        "notifications"
    ],
    "optional_permissions": [
        "<all_urls>"
    ],
    "background": {
        "persistent": false,
        "scripts": [
            "src/jquery.min.js",
            "src/md5.js",
            "src/app.js"
        ]
    },
    "options_ui": {
        "page": "options/options.html"
    },
    "icons": {
        "16": "icons/apple16.png",
        "32": "icons/apple32.png",
        "128": "icons/apple128.png",
        "256": "icons/apple256.png"
    },
    "default_locale": "de"
}
{
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "version": "2.1.1",
    "default_locale": "de",
    "permissions": [
        "contextMenus",
        "notifications",
        "storage"
    ],
    "optional_permissions": [
        "*://*/*"
    ],
    "background": {
        "scripts": [
            "src/md5.min.js",
            "src/app.js"
        ]
    },
    "options_ui": {
        "chrome_style": true,
        "page": "options/options.html"
    },
    "icons": {
        "16": "icons/apple16.png",
        "32": "icons/apple32.png",
        "128": "icons/apple128.png",
        "256": "icons/apple256.png"
    }
}